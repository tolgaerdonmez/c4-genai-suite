  "license": "Apache-2.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "test": "vitest run --coverage",
    "test:watch": "vitest",
    "build": "tsc && vite build",
    "format": "prettier --write \"src/**/*.{ts,tsx}\" \"test/**/*.ts\" --",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "find-unused-resources": "knip",
    "preview": "vite preview",
    "generate-api": "npx rimraf ./src/api/generated && npx @openapitools/openapi-generator-cli generate && node gen-helper.mjs",
    "generate-eval-api": "npx rimraf ./src/api/generated-eval && npx @openapitools/openapi-generator-cli generate --generator-key eval && node gen-helper.mjs"
  },
