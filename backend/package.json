  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start",
    "start:dev": "nest start --watch",
    "start:bundled": "cd ../frontend && npm run build && cd ../backend && npx rimraf ./assets && cp -r ../frontend/dist assets && nest start",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules\" jest --runInBand --forceExit",
    "test:cov": "NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules\" jest --runInBand --coverage --forceExit",
    "test:debug": "node --experimental-vm-modules --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "generate-openapi-dev": "npm run build && node dist/openapi.js -- localFile && npx openapi-filter -i --valid --flags x-external -- backend-dev-spec.json backend-external-spec.json && npx openapi-filter --flags x-external -- backend-dev-spec.json backend-dev-spec.json && npx prettier --write backend-*-spec.json",
    "generate-base": "npx @openapitools/openapi-generator-cli generate -g typescript-fetch --additional-properties=useSingleRequestParameter=false,ensureUniqueParams=false",
    "generate-tools": "npx rimraf ./src/extensions/tools/generated && npm run generate-base -- -i ./src/openapi/tools-spec.json -o ./src/extensions/tools/generated",
    "generate-reis": "npx rimraf ./src/domain/files/use-cases/generated && npm run generate-base -- -i ../services/reis/reis-dev-spec.json -o ./src/domain/files/use-cases/generated",
    "generate-executor": "npx rimraf ./src/domain/chat/middlewares/generated && npm run generate-base -- -i ./src/openapi/executor-spec.json -o ./src/domain/chat/middlewares/generated",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "migration:run": "npm run typeorm -- -d src/config/typeorm-migration.config.ts migration:run",
    "migration:format": "prettier --write src/migrations/*.ts",
    "migration:generate": "npm run typeorm -- -d src/config/typeorm-migration.config.ts migration:generate src/migrations/$npm_config_name $npm_config_options",
    "postmigration:generate": "npm run migration:format",
    "migration:generate:dryrun": "npm run typeorm -- -d src/config/typeorm-migration.config.ts migration:generate test --dr",
    "migration:create": "npm run typeorm -- migration:create src/migrations/$npm_config_name",
    "postmigration:create": "npm run migration:format",
    "migration:revert": "npm run typeorm -- -d src/config/typeorm-migration.config.ts migration:revert"
  },
